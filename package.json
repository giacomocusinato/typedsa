{
  "name": "typedsa",
  "version": "0.0.4",
  "description": "Data Structures and Algorithms in Typescript",
  "main": "dist/**/*",
  "types": "dist/**/*.d.ts",
  "prepublish": "tsc",
  "scripts": {
    "test": "",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^typedsa/ds$": "<rootDir>/lib/ds/index.ts",
      "^typedsa/algs$": "<rootDir>/lib/algs/index.ts",
      "^typedsa/errors$": "<rootDir>/lib/errors/index.ts",
      "^typedsa/utils$": "<rootDir>/lib/utils/index.ts"
    },
    "roots": [
      "<rootDir>/lib"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.2.4",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "rxjs": "^6.5.5"
  }
}
